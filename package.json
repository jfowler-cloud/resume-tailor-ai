{
  "name": "resume-tailor-project",
  "version": "1.0.0",
  "description": "AI-Powered Resume Tailor Platform using Claude 4.5",
  "main": "index.js",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "cdk": "cdk",
    "synth": "cdk synth --profile resume-tailor",
    "deploy": "cdk deploy --profile resume-tailor",
    "deploy:dev": "cdk deploy --profile resume-tailor --context environment=dev",
    "deploy:prod": "cdk deploy --profile resume-tailor --context environment=prod",
    "destroy": "cdk destroy --profile resume-tailor",
    "diff": "cdk diff --profile resume-tailor",
    "bootstrap": "cdk bootstrap --profile resume-tailor",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "lambda:install": "cd lambda && pip install -r requirements.txt -t .",
    "setup": "chmod +x setup/setup.sh && ./setup/setup.sh",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "keywords": [
    "aws",
    "cdk",
    "serverless",
    "ai",
    "claude",
    "resume",
    "bedrock",
    "step-functions"
  ],
  "author": "James Fowler <james.fowler.cloud@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "aws-cdk": "^2.1106.0",
    "eslint": "^10.0.0",
    "prettier": "^3.4.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.700.0",
    "@aws-sdk/client-dynamodb": "^3.990.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-ses": "^3.700.0",
    "@aws-sdk/client-sfn": "^3.700.0",
    "@aws-sdk/client-sts": "^3.700.0",
    "@aws-sdk/credential-providers": "^3.700.0",
    "@aws-sdk/lib-dynamodb": "^3.700.0",
    "aws-cdk-lib": "^2.170.0",
    "constructs": "^10.4.0",
    "dotenv": "^17.2.4"
  }
}
